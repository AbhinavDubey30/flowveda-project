{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "devCommand": "npm run dev:frontend",
  "installCommand": "npm run install-all",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
